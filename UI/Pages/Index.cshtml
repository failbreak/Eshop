@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}


<a href="~/User/Signup" > signup</a>
<a href="~/Admin/Create" > signup</a>
<a href="~/Admin/Edit" > signup</a>


    @foreach (var item in Model.Products)
    {
        <table>

        <tr>
            <th>@item.Name</th>
            @foreach (var picture in item.ProductPictures)
            {
            <th><img src="@picture.PictureUrl.FirstOrDefault()" alt="hello" /></th>
            }
            <th>@item.Manufacture</th>
            <th>@item.Price</th>
            
        </tr>
        </table>
    
    }
